{
  "name": "swa",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:azurite\" \"npm run dev:swa\"",
    "dev:swa": "WATCHPACK_POLLING=true CHOKIDAR_USEPOLLING=true swa start http://localhost:3000 --api-location api --host localhost --port 4280 --run \"next dev\"",
    "dev:azurite": "azurite",
    "build": "next build",
    "build:api": "cd api && npm install",
    "start": "next start",
    "swa:start": "swa start",
    "swa:build": "swa build",
    "swa:deploy": "swa deploy --env production"
  },
  "dependencies": {
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "next": "16.0.3"
  },
  "devDependencies": {
    "@azure/static-web-apps-cli": "^2.0.7",
    "typescript": "^5",
    "@types/node": "^24",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@tailwindcss/postcss": "^4",
    "tailwindcss": "^4"
  }
}
